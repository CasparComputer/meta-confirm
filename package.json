{
  "name": "meta-confirm",
  "version": "0.0.9",
  "description": "meta-confirm was intended to allow a simple way to test or confirm the existence of specific meta tag and description on a web page",
  "main": "meta-confirm.js",
  "engines": {
    "node": ">=0.10.3 <= 0.12.7"
  },
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --recursive --reporter spec --timeout 3000 && npm run coverage",
    "lint": "jshint **.js examples/**.js test/**.js",
    "env": "env",
    "coverage": "ISTANBUL_REPORTERS=text-summary,html istanbul instrument **.js --output ./lcov.info && istanbul cover **.js",    "prepublish": "npm install jshint --global # also runs npm run prebuild",
    "prebuild": "npm run test # also runs npm run pretest",
    "pretest": "jshint **.js examples/**.js test/**.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CasparComputer/meta-confirm"
  },
  "keywords": [
    "meta-tag",
    "test",
    "confirm",
    "express"
  ],
  "author": "Mike Caspar <mike@caspar.com> (http://www.caspar.com)",
  "bugs": {
    "url": "https://github.com/casparcomputer/meta-confirm/issues"
  },
  "license": "MIT",
  "dependencies": {
    "request": "~2.58.0"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "mocha": "^2.2.5",
    "istanbul": "~0.3.17",
    "mocha-istanbul": "~0.2.0"
  }
}
